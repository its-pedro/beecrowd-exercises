#include <stdio.h>

int main() {
    double volume, area, altura, diametro;
    const double pi = 3.14;

    while(scanf("%lf %lf", &volume, &diametro) !=EOF){
        area = ((diametro * diametro) / 4) * pi;
        altura = volume / area;
    
        printf("ALTURA = %.2lf\n", altura);
        printf("AREA = %.2lf\n", area);
        
    }
    return 0;
}
