#include<stdio.h>

int main(){
  int contador, num;

  while(num != 0){
    scanf("%d", &num);
    contador = 1; 

    while(contador <= num){
      if(num == contador){
        printf("%d\n", contador);
        
      }else{
        printf("%d ", contador);
      }
      contador++;
    }
  }
  return 0;
}
