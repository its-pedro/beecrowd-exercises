#include <stdio.h>

int main (){
    int c_1, c_2, c_3, c_4;

    scanf("%d %d %d %d", &c_1, &c_2, &c_3, &c_4);

    if(c_1 == 1){
        printf("1\n");
        
    }else if (c_2 == 1){
        printf("2\n");

    }else if(c_3 == 1){
        printf("3\n");

    }else if(c_4 == 1){
        printf("4\n");
        
    }
    return 0;
}
