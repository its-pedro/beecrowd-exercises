#include <stdio.h>

int main (){
    int posicao;
    scanf("%d", &posicao);

    if(posicao <= 1){
        printf("Top 1\n");
        
    }else if(posicao <= 3){
        printf("Top 3\n");

    }else if(posicao <= 5){
        printf("Top 5\n");

    }else if(posicao <= 10){
        printf("Top 10\n");

    }else if(posicao <= 25){
        printf("Top 25\n");

    }else if(posicao <= 50){
        printf("Top 50\n");

    }else if(posicao <= 100){
        printf("Top 100\n");

    }
    return 0;
}
